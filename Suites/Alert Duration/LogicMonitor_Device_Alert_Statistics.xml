            <value>/*******************************************************************************
 * © 2007-2023 - LogicMonitor, Inc. All rights reserved.
 ******************************************************************************/
import groovy.json.*

def durationPeriod = hostProps.get(&#34;alert.analysis.period&#34;) ?: hostProps.get(&#34;alert.duration.period&#34;) ?: &#34;7&#34; //Default to 7 day(s)
def durationMinimum = hostProps.get(&#34;alert.analysis.minimumTimeInSeconds&#34;) ?: hostProps.get(&#34;alert.duration.minimumTimeInSeconds&#34;) ?: &#34;0&#34; //Default to include alerts of any duration)
def deviceId = hostProps.get(&#34;system.deviceId&#34;)
def scriptCache
def maxAlertCacheSize = 10 //Number of scriptCache keys to utilize since max cache size is limited to around 20,000 alerts

/*Calculate start and end date for alert query*/
int timePeriod = 24 * durationPeriod.toInteger() //hours for rolling average
Date now = new Date()
long endEpoch = now.getTime() / 1000
long startEpoch = endEpoch - (timePeriod*3600)

def alerts
def devices

try {
    scriptCache = this.class.classLoader.loadClass(&#34;com.santaba.agent.util.script.ScriptCache&#34;).getCache();
    //String alertsCache = scriptCache.get(&#34;DeviceAlertStats-AlertsCache&#34;);
    String alertsCache = getAlertCache(maxAlertCacheSize,&#34;DeviceAlertStats-AlertsCache&#34;,scriptCache)
    if(alertsCache){
        /*Grab resources*/
        alerts = new JsonSlurper().parseText(alertsCache)
    }

    String resourceCache = scriptCache.get(&#34;DeviceAlertStats-ResourceCache&#34;);
    if(resourceCache){
        /*Grab resources*/
        devices = new JsonSlurper().parseText(resourceCache)
    }
}	
catch (Exception ex) {
	println ex
    return -1
}

if(alerts &#38;&#38; devices){
    def devicesWithAlerts = alerts.monitorObjectId.unique(false)
    devices.each { device -&#62; 
        if(devicesWithAlerts.contains(device.id)){
            def matching_alerts = alerts.findAll { it.monitorObjectId.toString() == device.id.toString()}

            matching_alerts.each { alert -&#62;
                double duration = alert.endEpoch - alert.startEpoch
                alert.put(&#34;Duration&#34;,duration)

                switch(alert.severity.toInteger()){
                    case 2:
                        //warning
                        alert.put(&#34;warnDuration&#34;,duration)
                        break;
                    case 3:
                        //error
                        alert.put(&#34;errorDuration&#34;,duration)
                        break;
                    case 4:
                        //critical
                        alert.put(&#34;criticalDuration&#34;,duration)
                        break;
                }
            }

            //Remove alerts not meeting minimum duration
            matching_alerts.removeAll { it.Duration &#60;= durationMinimum.toInteger()}

            def warnAlerts = matching_alerts.findAll { it.severity.toInteger() == 2}
            def errorAlerts = matching_alerts.findAll { it.severity.toInteger() == 3}
            def criticalAlerts = matching_alerts.findAll { it.severity.toInteger() == 4}

            calcDuration(matching_alerts,&#34;&#34;, device.id)
            calcDuration(warnAlerts,&#34;warn&#34;, device.id)
            calcDuration(errorAlerts,&#34;error&#34;, device.id)
            calcDuration(criticalAlerts,&#34;critical&#34;, device.id)
        }
        else{
            println &#34;${device.id}.longestAlertDuration=0&#34;
			println &#34;${device.id}.warnLongestAlertDuration=0&#34;
			println &#34;${device.id}.errorLongestAlertDuration=0&#34;
			println &#34;${device.id}.criticalLongestAlertDuration=0&#34;
            println &#34;${device.id}.alertSize=0&#34;
            println &#34;${device.id}.averageDuration=0&#34;
            println &#34;${device.id}.warnAverageDuration=0&#34;
            println &#34;${device.id}.errorAverageDuration=0&#34;
            println &#34;${device.id}.criticalAverageDuration=0&#34;
            println &#34;${device.id}.warnAlertSize=0&#34;
            println &#34;${device.id}.errorAlertSize=0&#34;
            println &#34;${device.id}.criticalAlertSize=0&#34;
            println &#34;${device.id}.averageDuration95th=0&#34;
            println &#34;${device.id}.warnAverageDuration95th=0&#34;
            println &#34;${device.id}.errorAverageDuration95th=0&#34;
            println &#34;${device.id}.criticalAverageDuration95th=0&#34;
        }
        println &#34;${device.id}.durationPeriod=${durationPeriod}&#34;
    }
}

return 0

def getAlertCache(cacheLimit,cacheName,cacheObject){
    def alerts = []
    def i = 0
    while(i &#60; cacheLimit){
        String alertsCache = cacheObject.get(&#34;${cacheName}-${i}&#34;);
        // println &#34;Getting cache index: ${i}&#34;
        if(alertsCache == null){
            break
        }
        else{
            def cachedAlerts = new JsonSlurper().parseText(alertsCache)
            alerts.addAll(cachedAlerts)
            i++
        }
    }
    // println &#34;Total cache size accross ${i} index(es), alert size ${alerts.size()}&#34;
    if(alerts){
        return new JsonBuilder( alerts ).toPrettyString()
    }
    else{
        // println &#34;No cache found&#34;
        return null
    }
}

def calcDuration(collection, dpPrefix, deviceId) {
    if(collection){
        println &#34;${deviceId}.${dpPrefix}AlertSize=${collection.size()}&#34;

        double duration = ((collection.&#34;${dpPrefix}Duration&#34;.sum() / collection.size()) / 60).round(2)
        println &#34;${deviceId}.${dpPrefix}AverageDuration=${duration}&#34;

        double duration95th =  getPercentile(collection.&#34;${dpPrefix}Duration&#34;, 0.95, dpPrefix, deviceId)
        println &#34;${deviceId}.${dpPrefix}AverageDuration95th=${duration95th}&#34;
    }
    else{
        println &#34;${deviceId}.${dpPrefix}AlertSize=0&#34;
        println &#34;${deviceId}.${dpPrefix}AverageDuration=0&#34;
        println &#34;${deviceId}.${dpPrefix}AverageDuration95th=0&#34;
        println &#34;${deviceId}.${dpPrefix}AverageDuration95th=0&#34;
        println &#34;${deviceId}.${dpPrefix}LongestAlertDuration=0&#34;
    }
}

Double getPercentile(collection, percentile, dpPrefix, deviceId) {
    def sortedCollection = collection.sort()

    double longestAlertDuration = (sortedCollection[-1] / 60).round(2)
    println &#34;${deviceId}.${dpPrefix}longestAlertDuration=${longestAlertDuration}&#34;

    int percentileIndex = Math.round(sortedCollection.size() * percentile)
    sortedCollection.subList(percentileIndex,sortedCollection.size()).clear()

    double result =  ((sortedCollection.sum() / Math.ceil(sortedCollection.size())) / 60).round(2)

    return result
}</value>
